script(type='text/javascript', src='https://cdn.jsdelivr.net/particles.js/2.0.0/particles.min.js')
style.
  .vcenter-item {
  display: flex;
  align-items: center;
  min-height: calc(100vh - 73.38px);
  }
  .innervcen {
  width:100%;
  }
  #clockdiv{
  font-family: sans-serif;
  color: #fff;
  display: inline-block;
  font-weight: 100;
  text-align: center;
  font-size: 30px;
  }
  #clockdiv > div{
  padding: 10px;
  border-radius: 3px;
  background: #292929;
  display: inline-block;
  }
  #clockdiv div > span{
  padding: 15px;
  border-radius: 3px;
  background: #303030;
  display: inline-block;
  }
  .smalltext{
  padding-top: 5px;
  font-size: 16px;
  }
  .button-hover:hover {
  border-color: #fff;
  }
  #particles-js {
  position: absolute;
  width: 100vw;
  height: calc(100vh - 73.38px);
  }
