#particles-js
  canvas.particles-js-canvas-el(style='width: 50%; height: 100%;')

.vcenter-item
  .innervcen
    .container
    center
        .card.mb-1
          .card-body
            p.text-center.mb-1
              img(src='/assets/img/logo.png' width='200' height='200')
            h5.text-center
              | Thank you for submitting your bot!
              p.mb-0.text-center.text-muted(style='font-size:12px;') You have added your bot to the list. Please wait for it to be verified.
            hr.bg-dark.ml-0.mb-4(style='max-width:600px;')
            .dansaidicant
              .g-daddy
                .g-recaptcha(data-size='invisible', data-theme='dark', data-sitekey='6LcGJPUZAAAAAE-JL52A3QLe8vwSbmXQxWRL7W23', data-callback='saveChanges')
            a.btn.btn-outline-tag(href='javascript:window.history.back()')
              i.fas.fa-arrow-left.text-damp.mr-2
              |  Back
            a.btn.btn-outline-tag(href='/')
              i.fas.fa-home.text-damp.mr-2
              |  Go Home

